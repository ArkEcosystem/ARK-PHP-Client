{
    "name": "arkecosystem/client",
    "description": "A simple PHP API client for the ARK Blockchain.",
    "keywords": ["api", "ark", "blockchain", "crypto", "currency"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://github.com/faustbrian"
    }],
    "require": {
        "php": "^7.1",
        "graham-campbell/guzzle-factory": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ArkEcosystem\\ArkClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArkEcosystem\\Tests\\ArkClient\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
